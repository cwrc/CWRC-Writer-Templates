            <HEADING SUBTYPE="3">Unpayed and volunteer work</HEADING>
            <CHRONSTRUCT> <DATERANGE></DATERANGE> <CHRONPROSE></CHRONPROSE> </CHRONSTRUCT>
            <HEADING SUBTYPE="3">Memberships</HEADING>
            <CHRONSTRUCT> <DATERANGE></DATERANGE> <CHRONPROSE></CHRONPROSE> </CHRONSTRUCT>
        </OCCUPATION>
